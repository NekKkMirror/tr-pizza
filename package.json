{
  "name": "pizza",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run --reporter verbose",
    "test:w": "vitest",
    "test:ui": "vitest --ui",
    "test:c": "vitest run --coverage",
    "test:cw": "vitest watch --coverage",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx,json}",
    "lint:styles": "stylelint",
    "lint:styles:all": "yarn lint:styles src/**/* --config ./.stylelintrc.json ",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md,json,scss} --config ./.prettierrc"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "ahooks": "^3.7.8",
    "axios": "^1.6.5",
    "clsx": "^2.1.0",
    "lodash.debounce": "^4.0.8",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-content-loader": "^6.2.1",
    "react-dom": "^18.2.0",
    "react-loadable": "^5.5.0",
    "react-paginate": "^8.2.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/htmlhint": "^1",
    "@types/jest": "^29.5.11",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "^20.11.4",
    "@types/qs": "^6",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/react-loadable": "^5.5.11",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "click-to-react-component": "^1.1.0",
    "cross-fetch": "^4.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-testing-library": "^6.2.0",
    "htmlhint": "^1.1.4",
    "jsdom": "^23.2.0",
    "msw": "^2.1.2",
    "prettier": "^3.2.2",
    "sass": "^1.69.7",
    "stylelint": "^16.2.0",
    "stylelint-config-idiomatic-order": "^10.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-svgr": "^4.2.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "packageManager": "yarn@4.0.2"
}
